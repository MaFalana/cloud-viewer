{
  "routes": [
    {
      "route": "/*",
      "allowedRoles": ["anonymous", "authenticated"]
    }
  ],
  "navigationFallback": {
    "rewrite": "/_fallback/index.html",
    "exclude": ["/assets/*", "/potree/*", "/index.html", "/viewer-example/*", "*.{css,scss,js,png,gif,ico,jpg,svg,wasm}"]
  },
  "mimeTypes": {
    ".json": "application/json",
    ".js": "text/javascript",
    ".css": "text/css",
    ".png": "image/png",
    ".jpg": "image/jpeg",
    ".svg": "image/svg+xml",
    ".wasm": "application/wasm"
  },
  "globalHeaders": {
    "cache-control": "public, max-age=3600"
  }
}
