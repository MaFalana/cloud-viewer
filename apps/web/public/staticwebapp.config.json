{
  "routes": [
    {
      "route": "/cloud-viewer",
      "rewrite": "/cloud-viewer/index.html"
    },
    {
      "route": "/cloud-viewer/*",
      "allowedRoles": ["anonymous", "authenticated"]
    }
  ],
  "navigationFallback": {
    "rewrite": "/cloud-viewer/_fallback/index.html",
    "exclude": ["/cloud-viewer/assets/*", "/cloud-viewer/potree/*", "/cloud-viewer/index.html", "/cloud-viewer/viewer-example/*", "*.{css,scss,js,png,gif,ico,jpg,svg,wasm}"]
  },
  "mimeTypes": {
    ".json": "application/json",
    ".js": "text/javascript",
    ".css": "text/css",
    ".png": "image/png",
    ".jpg": "image/jpeg",
    ".svg": "image/svg+xml",
    ".wasm": "application/wasm"
  },
  "globalHeaders": {
    "cache-control": "public, max-age=3600"
  }
}
